var itemsColumn = new Column({left: 0, right: 0, top: 100, bottom: 0, width: application.width, skin: lightgreySkin, style: textStyle,
	contents: [
	]
});

function updateItemsBody(allItemsDict){
	var data = {};
	for(item in allItemsDict){
		if(allItemsDict.hasOwnProperty(item)) {
			data = allItemsDict[item];
			itemsColumn.add(new Label({string: data.name}));
		}
	}
}

var itemsMainBody = new Container({top:50, bottom:0, skin:whiteSkin, 
			contents:[
				itemsColumn
			], 	
		});