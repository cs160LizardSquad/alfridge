var itemsColumn = new Column({top:100, bottom: 0, skin: lightgreySkin, style: textStyle,
	contents: [
	]
});

function updateItemsBody(allItemsDict){
	var data = {};
	for(item in allItemsDict){
		if(allItemsDict.hasOwnProperty(item)) {
			data = allItemsDict[item];
			itemsColumn.add(new Label({string: data.name}));
		}
	}
}

